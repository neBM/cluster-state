{
  "ID": "hello-world",
  "Name": "hello-world",
  "TaskGroups": [
    {
      "Name": "servers",
      "Tasks": [
        {
          "Name": "web",
          "Driver": "docker",
          "Config": {
            "image": "busybox:1",
            "command": "httpd",
            "args": [
              "-v",
              "-f",
              "-p",
              "${NOMAD_PORT_www}",
              "-h",
              "/local"
            ],
            "ports": [
              "www"
            ]
          },
          "Templates": [
            {
              "DestPath": "local/index.html",
              "EmbeddedTmpl": "<h1>Hello, Ben!</h1>\n"
            }
          ],
          "Resources": {
            "CPU": 50,
            "MemoryMB": 64
          }
        }
      ],
      "Networks": [
        {
          "DynamicPorts": [
            {
              "Label": "www",
              "To": 8001
            }
          ]
        }
      ],
      "Services": [
        {
          "PortLabel": "www",
          "Provider": "nomad"
        }
      ]
    }
  ]
}